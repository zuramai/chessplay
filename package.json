{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint \"**/*.{js,vue}\"",
    "lint:fix": "eslint --fix \"**/*.{js,vue}\"",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,vue}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "dart-sass": "^1.25.0",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "vue": "^3.0.5",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-eslint": "^8.0.1",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "prettier-eslint-cli": "^5.0.1",
    "stylelint": "^13.13.1",
    "stylelint-scss": "^3.20.1",
    "vite": "^2.4.0",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-windicss": "^1.2.4",
    "vue-auto-routing": "^1.0.1",
    "windicss": "^3.1.4"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*.scss": "stylelint --fix",
    "*.{js,vue,css,md}": "prettier --write"
  }
}
